{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@imcotton/bloom",
  "description": "Bloom filter implementation in pure typescript",
  "version": "201.5.1",
  "exports": {
    "./hashing/fnv1a": "./hashing/fnv1a.ts",
    "./hashing/fnv1a-deno": "./hashing/fnv1a-deno.ts",
    ".": "./mod.ts"
  },
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "lock": false,
  "publish": {
    "exclude": [
      "./.github",
      "./bloom.ts",
      "./bloom_origin_test.ts",
      ".editorconfig",
      "deno.lock"
    ]
  }
}

