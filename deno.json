{
  "$schema": "https://deno.land/x/deno/cli/schemas/config-file.v1.json",
  "name": "@imcotton/bloom",
  "description": "Bloom filter implementation in pure typescript",
  "version": "201.3.0",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno run --watch bloom.ts"
  },
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true
  },
  "lock": false,
  "publish": {
    "exclude": [
      "./.github",
      "./bloom.ts",
      ".editorconfig",
      "deno.lock"
    ]
  }
}

